<p id="notice"><%= notice %></p>

<h1>My GiveAways</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Start date</th>
      <th>End Date</th>
      <th>Participants</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @give_aways.each do |ga| %>
      <tr>
        <td><%= ga.name %></td>
        <td><%= ga.start %></td>
        <td><%= ga.end %></td>
        <td><%= link_to 'Show', ga %></td>
        <td><%= link_to 'Edit', edit_give_away_path(ga) %></td>
        <td><%= link_to 'Destroy', ga, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Create a Give Away', new_give_away_path %>
